<%= form_for(brands_path(@brand), method: 'get', role: "form") do |f| %>

    <div class="form-group">
      
      <%= f.email_field :id, placeholder: "Email Address" %>
    </div>

    <div class="form-group">
    
      <%= f.password_field :password, placeholder: "Password" %>
    </div> 

    <%= f.submit("Log In", :class => "btn btn-info round2")%>
  <% end %>