<%= form_tag '/login' do %>
    <div class="form-group">
      <%= label_tag :username%>
      <%= text_field_tag :username%>
    </div>
    <div class="form-group">
      <%= label_tag :password%>
      <%= password_field_tag :password%>
    </div> 
    <%= submit_tag "Log In", :class => "btn btn-info round2" %>
  <% end %>