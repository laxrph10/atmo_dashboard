<%= form_for(@brand, method: 'post') do |f| %>
  <%=f.label :company_representative_name%>
  <%=f.text_field :company_representative_name%><br>
  <%=f.label :email%>
  <%=f.email_field :email%><br>
  <%=f.label :secret_code%>
  <%=f.text_field :secret_code%></br>
  <%= f.submit("Validate your account.", :class => "btn btn-info round2")%>
  <% end %>
