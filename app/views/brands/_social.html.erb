<div class="brand_social">
  <table class="table">
    <thead>
      <tr>
        <td colspan="2"></td>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td><strong>Instagram</strong></td> 
        <td><i class="fa fa-instagram"></i></td>
        <td><%=@brand.instagrams.first.instagram_username%></td>
      </tr>
      <tr>
        <td><strong>Twitter</strong></td>
        <td><i class="fa fa-twitter"></i></td>
        <td><%=@brand.twitters.first.twitter_username%></td> 
      </tr>
      <tr>
        <td><strong>Soundcloud</strong></td>
        <td><i class="fa fa-soundcloud"></i></td>
        <td><%=@brand.soundclouds.first.soundcloud_username%></td>
      </tr>
      <tr>
        <td><strong>Pinterest</strong></td>
        <td><i class="fa fa-pinterest"></i></td>
        <td><%=@brand.pinterests.first.pinterest_username%></td>
      </tr>
