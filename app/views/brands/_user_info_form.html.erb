<%=form_for @brand do |f|%>
  <%=f.label :company_representative%>
  <%=f.text_field :company_representative_name%>
  <%=f.label :brand_name%>
  <%=f.text_field :brand_name%><br>
  <%=f.label :company%>
  <%=f.text_field :company_name%>
  <%=f.label :website%>
  <%=f.text_field :website%><br>
  <%=f.label :phone_number%>
  <%=f.text_field :phone_number%><br>
  <%=f.label :email%>
  <%=f.email_field :email%><br>
  <%=f.label :address%>
  <%=f.text_field :address%><br>
  <%=f.label :description%>
  <%=f.text_field :description%>
  <%=f.label :password%>
  <%=f.password_field :password%>
  <%=f.label :confirm_password%>
  <%=f.password_field :confirm_password%>
  <%=f.submit "Update", class: "brand_submit"%><br>
<%end%>
