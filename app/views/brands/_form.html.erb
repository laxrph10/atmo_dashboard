<%=form_for @brand, remote: true do |f|%>
  <%=f.label :brand_name%>
  <%=f.text_field :brand_name%><br>
  <%=f.label :company_name%>
  <%=f.text_field :company_name%>
  <%=f.label :website%>
  <%=f.text_field :website%><br>
  <%=f.label :phone%>
  <%=f.text_field :phone%><br>
  <%=f.label :email%>
  <%=f.email_field :email%><br>
  <%=f.label :company_representative_name%>
  <%=f.text_field :company_representative_name%><br>
  <%=f.label :address%>
  <%=f.text_field :address%><br>
  <%=f.label :description%>
  <%=f.text_field :description%><br>
  <%=f.label :avatar_img%>
  <%=f.text_field :avatar_img%><br>
  <%=f.label :header_img%>
  <%=f.text_field :header_img%><br>
<!--   <%=f.label :password%>
  <%=f.password_field :password%><br>
  <%=f.label :confirm_password%>
  <%=f.password_field :confirm_password%> -->
  <%=f.submit "Update", class: "brand_submit"%><br>
<%end%>
